        "mongodb-memory-server": "^10.2.1",
        "playwright": "^1.55.1",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "supertest": "^7.1.4",
        "tailwindcss": "^4.1.13",
        "typescript": "^5.9.2",
        "vite": "^7.1.7",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=22"
    }
}
