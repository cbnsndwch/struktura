{
  "name": "@cbnsndwch/struktura-main",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Main Struktura application - unified NestJS backend with React Router 7 admin UI",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:prod": "node dist/main.js",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest",
    "test:e2e": "vitest --config vitest.e2e.config.ts"
  },
  "dependencies": {
    "@cbnsndwch/struktura-contracts": "workspace:*",
    "@cbnsndwch/struktura-feature-shared": "workspace:*",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "apollo-server-express": "^3.12.0",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "mongoose": "^8.0.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@cbnsndwch/struktura-eslint-config": "workspace:*",
    "@cbnsndwch/struktura-tsconfig": "workspace:*",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^22.0.0",
    "eslint": "^9.36.0",
    "prettier": "^3.4.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22"
  }
}