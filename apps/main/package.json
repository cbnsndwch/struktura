{
    "name": "@cbnsndwch/struktura-main",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "description": "Main Struktura application - unified NestJS backend with React Router 7 admin UI",
    "scripts": {
        "build": "react-router build && nest build",
        "build:react-router": "react-router build",
        "build:nest": "nest build",
        "dev": "pnpm types && nest start --watch",
        "dev:react-router": "react-router dev",
        "start": "nest start",
        "start:prod": "node dist/main.js",
        "lint": "eslint . --fix",
        "format": "prettier --write .",
        "test": "vitest --run --passWithNoTests",
        "test:watch": "vitest",
        "test:unit": "vitest --run app/lib --passWithNoTests",
        "test:components": "vitest --run app/components --passWithNoTests",
        "test:features": "vitest --run app/features --passWithNoTests",
        "test:ci": "pnpm test:unit && pnpm test:components && pnpm test:features",
        "test:e2e": "vitest --config vitest.browser.config.ts --run",
        "test:e2e:watch": "vitest --config vitest.browser.config.ts",
        "test:e2e:ui": "vitest --config vitest.browser.config.ts --ui",
        "types": "react-router typegen && tsc"
    },
    "dependencies": {
        "@apollo/server": "^4.12.0",
        "@cbnsndwch/struktura-auth-contracts": "workspace:*",
        "@cbnsndwch/struktura-auth-domain": "workspace:*",
        "@cbnsndwch/struktura-collections-contracts": "workspace:*",
        "@cbnsndwch/struktura-collections-domain": "workspace:*",
        "@cbnsndwch/struktura-shared-contracts": "workspace:*",
        "@cbnsndwch/struktura-shared-domain": "workspace:*",
        "@cbnsndwch/struktura-shared-ui": "workspace:*",
        "@cbnsndwch/struktura-utils": "workspace:*",
        "@cbnsndwch/struktura-workspace-contracts": "workspace:*",
        "@cbnsndwch/struktura-workspace-domain": "workspace:*",
        "@hookform/resolvers": "^5.2.2",
        "@nestjs/apollo": "^13.1.0",
        "@nestjs/common": "^11.1.6",
        "@nestjs/core": "^11.1.6",
        "@nestjs/graphql": "^13.1.0",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/mongoose": "^11.0.3",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.1.6",
        "@nestjs/serve-static": "^5.0.3",
        "@nestjs/throttler": "^6.4.0",
        "@react-router/dev": "^7.9.3",
        "@react-router/express": "^7.9.3",
        "@react-router/node": "^7.9.3",
        "@types/react": "^19.1.15",
        "@types/react-dom": "^19.1.9",
        "bcrypt": "^6.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "express": "^5.1.0",
        "graphql": "^16.11.0",
        "graphql-subscriptions": "^3.0.0",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.544.0",
        "mongoose": "^8.18.2",
        "nodemailer": "^7.0.6",
        "passport": "^0.7.0",
        "passport-github2": "^0.1.12",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-hook-form": "^7.63.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "tw-animate-css": "^1.4.0",
        "zod": "^4"
    },
    "devDependencies": {
        "@cbnsndwch/struktura-eslint-config": "workspace:*",
