{
  "name": "@cbnsndwch/struktura-feature-shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Shared foundation layer for Struktura features",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./contracts": {
      "import": "./dist/contracts/index.js",
      "types": "./dist/contracts/index.d.ts"
    },
    "./domain": {
      "import": "./dist/domain/index.js", 
      "types": "./dist/domain/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui/index.js",
      "types": "./dist/ui/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cbnsndwch/struktura-contracts": "workspace:*"
  },
  "devDependencies": {
    "@cbnsndwch/struktura-eslint-config": "workspace:*",
    "@cbnsndwch/struktura-tsconfig": "workspace:*",
    "eslint": "^9.36.0",
    "prettier": "^3.4.1",
    "tsup": "^8.3.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22"
  }
}