{
    "name": "@cbnsndwch/struktura-shared-ui",
    "description": "Shared UI components for Struktura features",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist",
        "dev": "tsup --watch",
        "lint": "eslint . --fix",
        "format": "prettier --write .",
        "test": "vitest --run --passWithNoTests",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@cbnsndwch/struktura-shared-contracts": "workspace:*",
        "@radix-ui/react-dialog": "^1.1.3",
        "@radix-ui/react-dropdown-menu": "^2.1.5",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-select": "^2.1.5",
        "@radix-ui/react-separator": "^1.1.1",
        "@radix-ui/react-slot": "^1.1.1",
        "@radix-ui/react-switch": "^1.1.2",
        "@radix-ui/react-toast": "^1.2.5",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.544.0",
        "sonner": "^1.7.1",
        "tailwind-merge": "^3.3.1",
        "tw-animate-css": "^1.4.0"
    },
    "devDependencies": {
        "@cbnsndwch/struktura-eslint-config": "workspace:*",
        "@cbnsndwch/struktura-tsconfig": "workspace:*",
        "@types/react": "^19.1.15",
        "@types/react-dom": "^19.1.9",
        "eslint": "^9.36.0",
        "prettier": "^3.6.2",
        "tailwindcss": "^4.1.13",
        "tailwindcss-animate": "^1.0.7",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./globals.css": "./src/globals.css"
    },
    "engines": {
        "node": ">=22"
    }
}
