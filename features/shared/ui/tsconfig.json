{
    "extends": "@cbnsndwch/struktura-tsconfig/react-library",
    "compilerOptions": {
        "target": "ES2023",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "baseUrl": ".",
        "rootDir": "src",
        "outDir": "./dist",
        "jsx": "react-jsx",
        "paths": {
            "@/*": ["./src"]
        }
    },
    "include": ["src"],
    "exclude": ["dist", "node_modules"]
}
