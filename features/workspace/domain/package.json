{
    "name": "@cbnsndwch/struktura-workspace-domain",
    "description": "Workspace domain logic for Struktura",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist",
        "dev": "tsup --watch",
        "lint": "eslint . --fix",
        "format": "prettier --write .",
        "test": "vitest --run --passWithNoTests",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@cbnsndwch/struktura-auth-domain": "workspace:*",
        "@cbnsndwch/struktura-shared-domain": "workspace:*",
        "@cbnsndwch/struktura-workspace-contracts": "workspace:*",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "mongoose": "^8.18.2"
    },
    "devDependencies": {
        "@cbnsndwch/struktura-eslint-config": "workspace:*",
        "@cbnsndwch/struktura-tsconfig": "workspace:*",
        "eslint": "^9.36.0",
        "prettier": "^3.6.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    },
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    }
}
