{
    "name": "@cbnsndwch/struktura-collections-domain",
    "version": "0.1.0",
    "description": "Collections domain implementation for Struktura",
    "type": "module",
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf dist",
        "dev": "tsup --watch",
        "lint": "eslint . --fix",
        "format": "prettier --write .",
        "test": "vitest --run --passWithNoTests",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@cbnsndwch/struktura-collections-contracts": "workspace:*",
        "@cbnsndwch/struktura-shared-contracts": "workspace:*",
        "@cbnsndwch/struktura-schema-contracts": "workspace:*",
        "@nestjs/common": "^11.1.6",
        "@nestjs/core": "^11.1.6",
        "@nestjs/graphql": "^13.1.0",
        "@nestjs/mongoose": "^11.0.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "graphql": "^16.11.0",
        "jsep": "^1.4.0",
        "mongoose": "^8.18.2"
    },
    "devDependencies": {
        "@cbnsndwch/struktura-eslint-config": "workspace:*",
        "@cbnsndwch/struktura-tsconfig": "workspace:*",
        "@types/node": "^22.18.8",
        "eslint": "^9.36.0",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "@nestjs/common": "^11.0.0",
        "@nestjs/core": "^11.0.0"
    },
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    }
}
