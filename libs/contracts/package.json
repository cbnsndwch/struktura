{
    "name": "@cbnsndwch/struktura-contracts",
    "description": "Contracts and shared data types for Struktura",
    "version": "0.1.0",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "type": "module",
    "license": "MIT",
    "author": {
        "name": "cbnsndwch LLC",
        "url": "https://github.com/cbnsndwch"
    },
    "scripts": {
        "build": "tsup",
        "release": "pnpm test && pnpm build && changelogen --release --push && pnpm npm publish",
        "lint": "eslint . --fix",
        "test": "vitest --run --passWithNoTests",
        "ci:test": "vitest --run --passWithNoTests --bail 1",
        "test:watch": "vitest",
        "test:coverage": "vitest --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true"
    },
    "engines": {
        "node": ">=22"
    },
    "devDependencies": {
        "@cbnsndwch/struktura-eslint-config": "workspace:*",
        "@cbnsndwch/struktura-tsconfig": "workspace:*",
        "@commitlint/cli": "^20.0.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@nestjs/common": "^11.1.6",
        "@types/express": "^5.0.3",
        "@types/node": "^22",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "changelogen": "^0.6.2",
        "eslint": "^9.36.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.1",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "typings": ".dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./package.json": "./package.json"
    },
    "lint-staged": {
        "*.ts": "pnpm lint"
    }
}
